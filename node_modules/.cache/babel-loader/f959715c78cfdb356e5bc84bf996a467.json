{"ast":null,"code":"import { IonContent, IonHeader, IonPage, IonTitle, IonToolbar, IonInput, IonItem, IonLabel, IonList, IonButton } from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport axios from \"axios\";\nexport default defineComponent({\n  name: \"Home-Home\",\n  components: {\n    IonContent,\n    IonHeader,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonInput,\n    IonItem,\n    IonLabel,\n    IonList,\n    IonButton\n  },\n\n  data() {\n    return {\n      users: null,\n      codigo: \"\"\n    }; // sets users to null on instantiation\n  },\n\n  methods: {\n    loadUsers() {\n      axios.get(\"https://jsonplaceholder.typicode.com/users\").then(response => {\n        this.users = response.data; // assigns the data from api call to the users variable\n\n        console.log(this.users);\n      });\n    },\n\n    enter() {\n      console.log(\"Evento Enter\");\n    }\n\n  }\n});","map":{"version":3,"mappings":"AA0CA,SACEA,UADF,EAEEC,SAFF,EAGEC,OAHF,EAIEC,QAJF,EAKEC,UALF,EAMEC,QANF,EAOEC,OAPF,EAQEC,QARF,EASEC,OATF,EAUEC,SAVF,QAWO,YAXP;AAYA,SAASC,eAAT,QAAgC,KAAhC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAeD,eAAe,CAAC;EAC7BE,IAAI,EAAE,WADuB;EAE7BC,UAAU,EAAE;IACVb,UADU;IAEVC,SAFU;IAGVC,OAHU;IAIVC,QAJU;IAKVC,UALU;IAMVC,QANU;IAOVC,OAPU;IAQVC,QARU;IASVC,OATU;IAUVC;EAVU,CAFiB;;EAc7BK,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,IADF;MAELC,MAAM,EAAE;IAFH,CAAP,CADK,CAIF;EACJ,CAnB4B;;EAoB7BC,OAAO,EAAE;IACPC,SAAS,GAAG;MACVP,KAAI,CACDQ,GADH,CACO,4CADP,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKN,KAAL,GAAaM,QAAQ,CAACP,IAAtB,CADkB,CACU;;QAC5BQ,OAAO,CAACC,GAAR,CAAY,KAAKR,KAAjB;MACD,CALH;IAMD,CARM;;IASPS,KAAK,GAAG;MACNF,OAAO,CAACC,GAAR,CAAY,cAAZ;IACD;;EAXM;AApBoB,CAAD,CAA9B","names":["IonContent","IonHeader","IonPage","IonTitle","IonToolbar","IonInput","IonItem","IonLabel","IonList","IonButton","defineComponent","axios","name","components","data","users","codigo","methods","loadUsers","get","then","response","console","log","enter"],"sourceRoot":"","sources":["C:\\Users\\anu